{
  "manifest_version": 2,
  "name": "UT Sights",
  "description": "Get closer to the UT campus while distancing from home",
  "version": "1.3",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "icons": {
    "16": "img/16icon.png",
    "48": "img/64icon.png",
    "128": "img/128icon.png"
  }
}
